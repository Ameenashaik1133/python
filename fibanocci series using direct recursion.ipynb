    "#fibanocci series\n",
    "def fib(n):\n",
    "    if n<=1:\n",
    "        return n\n",
    "    else:\n",
    "        return fib(n-1)+fib(n-2)\n",
    "num=int(input(\"enter a number:\"))\n",
    "for i in range(num):\n",
    "    print(fib(i),end=' ')"
